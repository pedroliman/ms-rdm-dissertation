        <aux name="SwitchForShipmentsInForecast">
            <eqn>1</eqn>
            <display x="1087" y="651" color="blue" label_side="top" label_angle="81" />
        </aux>
        <aux name="IndustryVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>MAX(InitialDemandForecast,&#x0A;SwitchForShipmentsInForecast*IndustryShipments+&#x0A;(1-SwitchForShipmentsInForecast)*IndustryOrderRate)</eqn>
            <display x="1015" y="698" color="blue" />
        </aux>
        <aux name="VolumeReportingDelay">
            <eqn>0.25</eqn>
            <display x="1102" y="690" color="blue" />
        </aux>
        <aux name="ReportedIndustryVolume" flow_concept="true">
