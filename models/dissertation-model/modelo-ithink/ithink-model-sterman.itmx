    <isee:equation_prefs order_by="module" />
    <model>
        <stock name="NPVProfit">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0</eqn>
            <inflow>NPVProfitChange</inflow>
            <display x="261" y="82" color="#009802" />
        </stock>
        <flow name="NPVProfitChange">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>NetIncome*DiscountFactor</eqn>
            <display x="191" y="81" color="#009802">
                <pts>
                    <pt x="144" y="81" />
                    <pt x="239" y="81" />
                </pts>
            </display>
        </flow>
        <aux name="DiscountRate">
            <eqn>0.04</eqn>
            <display x="121" y="129" color="#009802" />
        </aux>
        <aux name="DiscountFactor">
            <eqn>EXP(-DiscountRate*Time)</eqn>
            <display x="183" y="123" color="#009802" />
        </aux>
        <aux name="NetIncome" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Revenue-Cost</eqn>
            <display x="273" y="133" color="blue" />
        </aux>
        <aux name="NPVIndustryProfits">
            <eqn>SUM(NPVProfit[*])</eqn>
            <display x="329" y="61" color="#009802" />
        </aux>
        <aux name="Cost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>VariableCost+FixedCost</eqn>
            <display x="333" y="126" color="blue" />
        </aux>
        <aux name="FixedCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Capacitys*UnitFixedCost</eqn>
            <display x="393" y="101" color="blue" />
        </aux>
        <aux name="VariableCost" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Shipments*UnitVariableCost</eqn>
            <display x="409" y="143" color="blue" />
        </aux>
        <stock name="ValueOfBacklog">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <inflow>ValueOfNewOrders</inflow>
            <outflow>Revenue</outflow>
            <display x="265" y="209" color="blue" />
        </stock>
        <flow name="Revenue">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Shipments*AveragePriceofOrderBook</eqn>
            <non_negative />
            <display x="341" y="210" color="blue">
                <pts>
                    <pt x="287" y="210" />
                    <pt x="395" y="210" />
                </pts>
            </display>
        </flow>
        <flow name="ValueOfNewOrders">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Price*Orders</eqn>
            <non_negative />
            <display x="203" y="213" color="blue">
                <pts>
                    <pt x="163" y="213" />
                    <pt x="243" y="213" />
                </pts>
            </display>
        </flow>
        <aux name="AveragePriceOfOrderBook">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>ValueOfBacklog/Backlog&#x0A;&#x0A;</eqn>
            <display x="318" y="255" color="blue" />
        </aux>
        <stock name="Backlog">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <inflow>Orders</inflow>
            <outflow>Shipments</outflow>
            <display x="573" y="521" color="blue" />
        </stock>
        <flow name="Orders">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>IndustryOrderRate*OrderShare</eqn>
            <non_negative />
            <display x="520" y="523" color="blue">
                <pts>
                    <pt x="489" y="523" />
                    <pt x="551" y="523" />
                </pts>
            </display>
        </flow>
        <flow name="Shipments">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>{Se o Switch esta ligado}&#x0A;SwitchForCapacity*MIN(DesiredShipments[Company],Capacitys[Company]) + &#x0A;{Se nao esta ligado}&#x0A;(1-SwitchForCapacity)*DesiredShipments</eqn>
            <non_negative />
            <display x="644" y="522" color="blue">
                <pts>
                    <pt x="595" y="522" />
                    <pt x="693" y="522" />
                </pts>
            </display>
        </flow>
        <stock name="CumulativeAdopters">
            <eqn>InitialCumulativeAdopters</eqn>
            <inflow>AdoptionRate</inflow>
            <display x="302" y="592" color="blue" />
        </stock>
        <flow name="AdoptionRate">
            <eqn>NonAdopters*&#x0A;(InnovatorAdoptionFraction + WOMStrength*CumulativeAdopters/Population)</eqn>
            <non_negative />
            <display x="226" y="589" color="blue">
                <pts>
                    <pt x="173" y="589" />
                    <pt x="280" y="589" />
                </pts>
            </display>
        </flow>
        <stock name="InstalledBase">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InitialInstalledBase</eqn>
            <inflow>Shipments</inflow>
            <outflow>DiscardRate</outflow>
            <display x="715" y="525" color="blue" />
        </stock>
        <flow name="DiscardRate">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InstalledBase*FractionalDiscardRate</eqn>
            <non_negative />
            <display x="780" y="524" color="blue">
                <pts>
                    <pt x="737" y="524" />
                    <pt x="823" y="524" />
                </pts>
            </display>
        </flow>
        <array name="Price">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <stock subscript="Company1">
                <eqn>9000</eqn>
            </stock>
            <stock subscript="Company2">
                <eqn>9000</eqn>
            </stock>
            <inflow>ChangeInPrice</inflow>
            <display x="982" y="1168" color="blue" />
        </array>
        <flow name="ChangeInPrice">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0</eqn>
            <display x="918" y="1167" color="blue">
                <pts>
                    <pt x="877" y="1167" />
                    <pt x="960" y="1167" />
                </pts>
            </display>
        </flow>
        <stock name="CumulativeProduction">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InitialProductionExperience</eqn>
            <inflow>Production</inflow>
            <display x="1041" y="164" color="blue" />
        </stock>
        <flow name="Production">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Shipments</eqn>
            <display x="976" y="165" color="blue">
                <pts>
                    <pt x="933" y="165" />
                    <pt x="1019" y="165" />
                </pts>
            </display>
        </flow>
        <aux name="Capacitys">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>10000000</eqn>
            <display x="218" y="799" color="blue" />
        </aux>
        <aux name="UnitFixedCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InitialUnitFixedCost*Learning</eqn>
            <display x="1183" y="76" color="blue" />
        </aux>
        <aux name="UnitVariableCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InitialUnitVariableCost*Learning</eqn>
            <display x="1212" y="307" color="blue" />
        </aux>
        <aux name="DeliveryDelay" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Backlog/Shipments</eqn>
            <display x="617" y="485" color="blue" label_side="top" label_angle="94" />
        </aux>
        <aux name="FractionalDiscardRate">
            <eqn>0.1</eqn>
            <display x="807" y="452" color="blue" label_side="top" label_angle="99" />
        </aux>
        <aux name="NormalDeliveryDelay">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.25</eqn>
            <display x="521" y="574" color="blue" />
        </aux>
        <aux name="DesiredShipments">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Backlog/NormalDeliveryDelay</eqn>
            <display x="612" y="568" color="blue" />
        </aux>
        <aux name="SwitchForCapacity">
            <eqn>1</eqn>
            <display x="663" y="591" color="blue" label_angle="261" />
        </aux>
        <flow name="IndustryOrderRate">
            <eqn>InitialOrderRate+ReorderRate</eqn>
            <non_negative />
            <display x="986" y="513" color="blue">
                <pts>
                    <pt x="937" y="513" />
                    <pt x="1036" y="513" />
                </pts>
            </display>
        </flow>
        <aux name="OrderShare">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Attractiveness/TotalAttractiveness</eqn>
            <display x="644" y="1309" color="blue" />
        </aux>
        <aux name="Population">
            <eqn>100000000</eqn>
            <display x="145" y="534" color="blue" />
        </aux>
        <aux name="WOMStrength">
            <eqn>1</eqn>
            <display x="308" y="628" color="blue" />
        </aux>
        <aux name="InnovatorAdoptionFraction">
            <eqn>0.001</eqn>
            <display x="128" y="628" color="blue" />
        </aux>
        <aux name="LowestPrice">
            <eqn>MIN(Price[*])</eqn>
            <display x="302" y="454" color="blue" />
        </aux>
        <aux name="ReferencePrice">
            <eqn>1000</eqn>
            <display x="225" y="447" color="blue" label_side="top" label_angle="94" />
        </aux>
        <aux name="ReferenceIndustryDemandElasticity">
            <eqn>1000</eqn>
            <display x="112" y="459" color="blue" label_side="top" label_angle="99" />
        </aux>
        <aux name="ReferencePopulation">
            <eqn>60000000</eqn>
            <display x="81" y="503" color="blue" label_angle="257" />
        </aux>
        <aux name="DemandCurveSlope">
            <eqn>(ReferencePopulation*ReferenceIndustryDemandElasticity)/(ReferencePrice)</eqn>
            <display x="180" y="480" color="blue" label_angle="249" />
        </aux>
        <aux name="IndustryDemand">
            <eqn>MIN(Population,&#x0A;ReferencePopulation* MAX(0,1+DemandCurveSlope&#x0A;&#x09;*(LowestPrice&#x0A;&#x09;-ReferencePrice)/ReferencePopulation))</eqn>
            <display x="239" y="513" color="blue" />
        </aux>
        <aux name="InitialDiffusionFraction">
            <eqn>0.001</eqn>
            <display x="362" y="488" color="blue" />
        </aux>
        <aux name="InitialCumulativeAdopters">
            <eqn>InitialDiffusionFraction*IndustryDemand&#x0A;&#x0A;{No R, este valor é difícil de calcular na &quot;largada&quot; do modelo. Calibrar com os dados, e inserir valor manualmente, pelo menos por enquanto. Um cuidado especial é que as incertezas teoricamente não podem interferir neste calculo.}</eqn>
            <display x="343" y="530" color="red" />
        </aux>
        <aux name="NonAdopters">
            <eqn>IndustryDemand - CumulativeAdopters&#x0A;&#x0A;{Esta Variável está modelada no modelo original como um estoque. Verificar se há diferenca de calculo devido a ela.}</eqn>
            <display x="101" y="585" color="red" />
        </aux>
        <aux name="UnitsPerHousehold">
            <eqn>1</eqn>
            <display x="1169" y="458" color="blue" />
        </aux>
        <aux name="InitialOrderRate" flow_concept="true">
            <eqn>AdoptionRate*UnitsPerHousehold</eqn>
            <display x="1121" y="505" color="blue" label_side="right" label_angle="356" />
        </aux>
        <flow name="ReorderRate">
            <eqn>SUM(DiscardRate)</eqn>
            <non_negative />
            <display x="990" y="459" color="blue">
                <pts>
                    <pt x="940" y="459" />
                    <pt x="1040" y="459" />
                </pts>
            </display>
        </flow>
        <aux name="InitialInstalledBase">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>InitialCumulativeAdopters*UnitsPerHousehold*InitialSharePlayers</eqn>
            <display x="731" y="606" color="blue" />
        </aux>
        <aux name="NPLAYERS">
            <eqn>SIZE(NPVProfit)</eqn>
            <display x="86" y="59" color="blue" />
        </aux>
        <aux name="InitialSharePlayers">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.5</eqn>
            <display x="678" y="641" color="blue" />
        </aux>
        <aux name="InitialDemandForecast" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>ReorderRate</eqn>
            <display x="1009" y="646" color="blue" />
        </aux>
        <aux name="IndustryShipments" flow_concept="true">
            <eqn>sum(Shipments[*])</eqn>
            <display x="732" y="471" color="blue" label_side="top" label_angle="86" />
        </aux>
        <aux name="SwitchForShipmentsInForecast">
            <eqn>1</eqn>
            <display x="1087" y="651" color="blue" label_side="top" label_angle="81" />
        </aux>
        <aux name="IndustryVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>MAX(InitialDemandForecast[Company],&#x0A;SwitchForShipmentsInForecast*IndustryShipments+&#x0A;(1-SwitchForShipmentsInForecast)*IndustryOrderRate)</eqn>
            <display x="1015" y="698" color="blue" />
        </aux>
        <aux name="VolumeReportingDelay">
            <eqn>0.25</eqn>
            <display x="1098" y="715" color="blue" label_side="top" label_angle="73" />
        </aux>
        <aux name="LaggedIndustryVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>DELAY(ReportedIndustryVolume,TimeForHistoricalVolume,ReportedIndustryVolume)</eqn>
            <display x="1002" y="834" color="#008F44" />
        </aux>
        <aux name="TimeForHistoricalVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="1024" y="898" color="#008F44" />
        </aux>
        <aux name="ExpGrowthInVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>LN(ReportedIndustryVolume[Company]/LaggedIndustryVolume[Company])&#x0A;/&#x0A;TimeForHistoricalVolume</eqn>
            <display x="1092" y="867" color="blue" />
        </aux>
        <aux name="ForecastHorizon" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="1138" y="933" color="blue" />
        </aux>
        <aux name="CapacityAcquisitionDelay" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="1258" y="806" color="blue" />
        </aux>
        <aux name="ExpectedIndustryDemand" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>ReportedIndustryVolume*EXP(ForecastHorizon[Company]*CapacityAcquisitionDelay[Company]*ExpGrowthInVolume[Company])</eqn>
            <display x="1175" y="828" color="blue" />
        </aux>
        <aux name="LaggedVolumeForecast" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>DELAY(ExpectedIndustryDemand,CapacityAcquisitionDelay,ExpectedIndustryDemand)</eqn>
            <display x="1213" y="690" color="blue" />
        </aux>
        <aux name="ForecastError" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(LaggedVolumeForecast-IndustryVolume)/(1e-009+IndustryVolume)</eqn>
            <display x="1151" y="652" color="blue" />
        </aux>
        <stock name="ReportedIndustryVolume">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>IndustryVolume</eqn>
            <inflow>change</inflow>
            <display x="1089" y="777" color="blue" />
        </stock>
        <flow name="change">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(IndustryVolume-ReportedIndustryVolume)/VolumeReportingDelay</eqn>
            <display x="1024" y="776" color="blue">
                <pts>
                    <pt x="982" y="776" />
                    <pt x="1067" y="776" />
                </pts>
            </display>
        </flow>
        <aux name="LCStrength">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.7</eqn>
            <display x="1038" y="57" color="blue" />
        </aux>
        <aux name="LCExponent">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>LN(LCStrength)/LN(2)</eqn>
            <display x="1099" y="96" color="blue" />
        </aux>
        <aux name="Learning">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(CumulativeProduction/InitialProductionExperience)^LCExponent</eqn>
            <display x="1133" y="166" color="blue" />
        </aux>
        <aux name="InitialProductionExperience">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>10000000</eqn>
            <display x="1075" y="221" color="blue" />
        </aux>
        <aux name="RatioOfFixedToVarCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.3</eqn>
            <display x="1208" y="173" color="blue" />
        </aux>
        <aux name="InitialPrice">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1000</eqn>
            <display x="1297" y="165" color="blue" />
        </aux>
        <aux name="NormalCapacityUtilization">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.8</eqn>
            <display x="1376" y="208" color="blue" />
        </aux>
        <aux name="NormalProfitMargin">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.2</eqn>
            <display x="1374" y="142" color="blue" />
        </aux>
        <aux name="InitialUnitVariableCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(InitialPrice/(1+NormalProfitMargin))*(1/(1+RatioOfFixedToVarCost/NormalCapacityUtilization))</eqn>
            <display x="1271" y="220" color="blue" />
        </aux>
        <aux name="InitialUnitFixedCost">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(InitialPrice/(1+NormalProfitMargin))*RatioOfFixedToVarCost*(1/(1+RatioOfFixedToVarCost/NormalCapacityUtilization))</eqn>
            <display x="1279" y="104" color="blue" />
        </aux>
        <aux name="MarketShare" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>Shipments/IndustryShipments&#x0A;</eqn>
            <display x="658" y="422" color="blue" />
        </aux>
        <aux name="ReferenceDeliveryDelay">
            <eqn>0.25</eqn>
            <display x="507" y="1187" color="blue" />
        </aux>
        <aux name="SensOfAttractToAvailability">
            <eqn>-4</eqn>
            <display x="511" y="1253" color="blue" />
        </aux>
        <aux name="SensOfAttractToPrice">
            <eqn>-8</eqn>
            <display x="739" y="1266" color="blue" />
        </aux>
        <aux name="AttractivenessFromPrice">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>EXP(SensOfAttractToPrice*(Price[Company]/ReferencePrice&#x0A;))</eqn>
            <display x="685" y="1219" color="blue" />
        </aux>
        <aux name="AttractivenessFromAvailability">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>EXP(SensOfAttractToAvailability*(DeliveryDelay[Company]/ReferenceDeliveryDelay))</eqn>
            <display x="578" y="1201" color="blue" />
        </aux>
        <aux name="Attractiveness">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>AttractivenessFromPrice*AttractivenessFromAvailability</eqn>
            <display x="622" y="1263" color="blue" />
        </aux>
        <aux name="TotalAttractiveness">
            <eqn>SUM(Attractiveness)</eqn>
            <display x="531" y="1311" color="blue" />
        </aux>
        <aux name="TimeToPerceiveCompTargetCapacity">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>0.25</eqn>
            <display x="468" y="904" color="blue" label_angle="279" />
        </aux>
        <aux name="CompetitorTargetCapacity">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>IndustryTotalTargetCapacity-TargetCapacity</eqn>
            <display x="489" y="777" color="blue" />
        </aux>
        <aux name="ExpectedCompCapacity">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>NormalCapacityUtilization*(WeightOnSupplyLine*PerceivedCompTargetCapacity+(1-WeightOnSupplyLine)*CompetitorCapacity)</eqn>
            <display x="589" y="993" color="blue" />
        </aux>
        <aux name="UncontestedDemand">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>MAX(0,ExpectedIndustryDemand[Company] - ExpectedCompCapacity[Company])</eqn>
            <display x="773" y="958" color="blue" />
        </aux>
        <aux name="CompetitorCapacity">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>IndustryCapacity-Capacitys</eqn>
            <display x="525" y="956" color="blue" />
        </aux>
        <aux name="IndustryCapacity">
            <eqn>SUM(Capacitys)</eqn>
            <display x="470" y="1009" color="blue" />
        </aux>
        <aux name="WeightOnSupplyLine">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="794" y="1008" color="blue" />
        </aux>
        <aux name="UncontestedMarketShare">
            <dimensions>
                <dim name="Company" />
