            </dimensions>
            <eqn>SMTH1(IndustryVolume,VolumeReportingDelay)</eqn>
            <display x="1058" y="738" color="#008F44" />
        </aux>
        <aux name="LaggedIndustryVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>DELAY(ReportedIndustryVolume,TimeForHistoricalVolume,ReportedIndustryVolume)</eqn>
            <display x="975" y="778" color="#008F44" />
        </aux>
        <aux name="TimeForHistoricalVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="957" y="832" color="#008F44" />
        </aux>
        <aux name="ExpGrowthInVolume" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>LN(ReportedIndustryVolume[Company]/LaggedIndustryVolume[Company])&#x0A;/&#x0A;TimeForHistoricalVolume</eqn>
            <display x="1058" y="800" color="blue" />
        </aux>
        <aux name="ForecastHorizon" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="1159" y="733" color="blue" />
        </aux>
        <aux name="CapacityAcquisitionDelay" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>1</eqn>
            <display x="1186" y="830" color="blue" />
        </aux>
        <aux name="ExpectedIndustryDemand" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>ReportedIndustryVolume*EXP(ForecastHorizon[Company]*CapacityAcquisitionDelay[Company]*ExpGrowthInVolume[Company])</eqn>
            <display x="1159" y="787" color="blue" />
        </aux>
        <aux name="LaggedVolumeForecast" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>DELAY(ExpectedIndustryDemand,CapacityAcquisitionDelay,ExpectedIndustryDemand)</eqn>
            <display x="1213" y="690" color="blue" />
        </aux>
        <aux name="ForecastError" flow_concept="true">
            <dimensions>
                <dim name="Company" />
            </dimensions>
            <eqn>(LaggedVolumeForecast-IndustryVolume)/(1e-009+IndustryVolume)</eqn>
            <display x="1151" y="652" color="blue" />
        </aux>
