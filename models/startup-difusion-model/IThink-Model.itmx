        </aux>
        <aux name="ShareDemanda">
            <eqn>0.3</eqn>
            <display x="484" y="164" color="blue" label_side="top" label_angle="86" />
        </aux>
        <flow name="TransacoesDemandadas">
            <eqn>DemandaUnitaria*ConsumidoresAtivos</eqn>
            <non_negative />
            <display x="334" y="139" color="blue">
